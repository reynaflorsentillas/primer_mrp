<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="primer_view_stock_move_tree" model="ir.ui.view">
            <field name="name">primer.mrp.stock.move.tree</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.stock_move_tree" />
            <field name="arch" type="xml">

                <xpath expr="//field[@name='date_expected']" position="replace">
                    <field name="date" />
                </xpath>

                
            </field>
        </record>

    </data>
</odoo>